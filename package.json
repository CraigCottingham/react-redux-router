{
  "name": "@rentpath/react-redux-router",
  "version": "3.5.0",
  "author": "Ari Bouius <abouius@rentpath.com>",
  "main": "./lib/index.js",
  "module": "es/index.js",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf lib && rm -rf es",
    "build": "yarn run clean && yarn run build:es && yarn run build:commonjs",
    "build:es": "BABEL_ENV=es babel --ignore 'src/**/__tests__/*' src -d es",
    "build:commonjs": "BABEL_ENV=commonjs babel --ignore 'src/**/__tests__/*' src -d lib",
    "test": "BABEL_ENV=commonjs NODE_ENV=development mocha",
    "test:watch": "yarn run test --watch",
    "lint": "eslint src",
    "lint:fix": "yarn run lint -- --fix",
    "prepublish": "yarn test && yarn lint && yarn build"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/plugin-transform-destructuring": "^7.4.3",
    "@babel/plugin-transform-modules-commonjs": "^7.4.3",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.0",
    "babel-eslint": "^10.0.1",
    "chai": "^4.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-react": "^7.12.4",
    "fast-async": "^7",
    "jsdom": "^15.0.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^6.1.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.2",
    "react-test-renderer": "^16.8.6",
    "redux": "^4.0.1",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.3",
    "history": "^4.9.0",
    "little-router": "2.3.2",
    "parse-location": "^1.0.5",
    "prop-types": "^15.7.2"
  },
  "peerDependencies": {
    "react": "^16.5.0",
    "react-redux": "^4.0.0 || ^5.0.0",
    "redux": "^3.7.2"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
