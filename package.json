{
  "name": "@rentpath/react-redux-router",
  "version": "1.1.0",
  "author": "Ari Bouius <abouius@rentpath.com>",
  "main": "./lib/index.js",
  "module": "es/index.js",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf lib && rm -rf es",
    "build": "yarn run clean && yarn run build:es && yarn run build:commonjs",
    "build:es": "BABEL_ENV=es babel --ignore __tests__ -d es src",
    "build:commonjs": "BABEL_ENV=commonjs babel --ignore __tests__ -d lib src",
    "test": "BABEL_ENV=commonjs NODE_ENV=development mocha --compilers js:babel-core/register --recursive",
    "test:watch": "yarn run test -- --watch",
    "lint": "eslint src",
    "lint:fix": "yarn run lint -- --fix"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "chai": "^3.5.0",
    "enzyme": "^2.7.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.1.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.10.3",
    "jsdom": "^10.1.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^3.2.0",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.4",
    "react-test-renderer": "^15.5.4",
    "redux": "^3.6.0",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.10.0"
  },
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "history": "^4.6.1",
    "little-router": "2.3.2",
    "parse-location": "^1.0.5",
    "prop-types": "^15.5.8"
  },
  "peerDependencies": {
    "react": "^15.3.1",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0"
  }
}
