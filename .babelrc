{
  "presets": [
    ["es2015", { "modules": false }],
    "react"
  ],
  "plugins": [
    "transform-runtime",
    "transform-class-properties",
    "babel-plugin-transform-object-rest-spread",
    "transform-async-to-generator"
  ],
  "env": {
    "commonjs": {
      "plugins": [
        ["transform-es2015-modules-commonjs", { "loose": true }]
      ]
    },
  }
}
